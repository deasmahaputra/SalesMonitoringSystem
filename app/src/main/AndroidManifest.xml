<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.buahbatu.toyotasalesman">

    <!-- Internet Permission -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <!-- Location Permission -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <!-- IMEI Read -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <!--LOAD MAP-->
    <permission android:name="com.buahbatu.toyotasalesman.TrackingActivity.permission.MAPS_RECEIVE" android:protectionLevel="signature"/>
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyCS8Za5B-OzQh1Yj6ROSzrs0vlrWYj8A5w"/>
            <!--android:value="AIzaSyDX7Vcg_tm5w0zCdeiJfTRufghuz3ftVSQ" />-->
        <service
            android:name=".services.ReportingService3"
            android:icon="@mipmap/ic_launcher"
            android:exported="false"
            android:label="@string/service_name">
        </service>
        <service
            android:name=".services.ReportingService"
            android:exported="false" />
        <service
            android:name=".services.TesService"
            android:exported="false" />

        <receiver android:name=".services.AutoStart">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Main2Activity"
            android:label="@string/app_name">

        </activity>
        <activity android:name=".TrackingActivity"/>
    </application>

</manifest>